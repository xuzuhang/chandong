/* pages/common/enterImg/enterImg.wxss */
page{
  width: 100%;
  height: 100%;
}
.wrapper{
  width: 100%;
  height: 100%;
  position: relative;
}
image{
  width: 100%;
  height: 100%;
}
.btn{
  height: 60rpx;
  line-height: 60rpx;
  border-radius:30rpx;
  width: 200rpx;
  text-align: center;
  font-size:25rpx; 
  position: absolute;
  top: 30rpx;
  right:30rpx;
  color:#fff;
  border: 4rpx solid #f47c01;
  background:transparent;
}
 button[plain]{
  color:#fff;
  border: 4rpx solid #f47c01;
} 
button[disabled]{
  color:#fff!important;
  border: 4rpx solid #f47c01!important;
  background:transparent!important;
}