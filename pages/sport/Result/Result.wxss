  background: #fff;
  border-radius:25rpx; 
  margin: 25rpx auto 17rpx;
  text-align: center;
}
.block-list{
 height: 210rpx;
 border-bottom: 0.5rpx solid #e6e6e6;
  width: 604rpx;
  margin: 0 auto;
}
.Btop{
  display: flex;
  justify-content: space-between;
  margin: 23rpx auto 35rpx;
  font-size: 30rpx;
  font-weight: 600;
  color: #404040;
}
.Bmid{
  display:flex;
  justify-content: space-between;
  color: #999;
  font-size: 28rpx;
}
.Bmid>view{
  flex: 1;
  text-align: center;
  margin: 5rpx;
}
.Bbot{
  position: relative;
}
.Bbot .progress{
  height:20rpx;
   border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  background: #e6e6e6;
}
.Bbot .progress>view{
  height:20rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  background: #fd8d0c;
  transition: all 0.8s linear;
}
.Bbot .ff{
  color: #999;
  font-size: 18rpx;
  position: absolute;
  top: 0;
  left: 33.3%;
}
.Bbot .ff>view:nth-of-type(1){
  width: 5rpx;
  height: 20rpx;
  background: #fff;
}
.Bbot .ff>view:nth-of-type(2){
  text-indent: -0.5em;
}
.Bbot .ff.f2{
  left: 66.6%;
}
.block-list:nth-of-type(5) .Bbot .ff{
    left: 50%;
}